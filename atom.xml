---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Undesigned</title>
 <link href="http://undesigned.io/atom.xml" rel="self"/>
 <link href="http://undesigned.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://undesigned.io/</id>
 <author>
   <name>Undesigned</name>
   <email>hello@undesigned.io</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://undesigned.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://undesigned.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>